				#else
				float3 defaultVertexValue = float3( 0, 0, 0 );
				#endif
				float3 vertexValue = ( inputMesh.normalOS * ( ( sin( ( mulTime28 + ( ase_worldPos.x + ase_worldPos.z ) ) ) * _MotionRange ) * tex2DNode47.r * tex2DNode47.g * tex2DNode47.b ) );

